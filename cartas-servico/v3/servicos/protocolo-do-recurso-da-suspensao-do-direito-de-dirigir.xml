        <ate max="999" unidade="dias-uteis"/>
        <descricao/>
    </tempo-total-estimado>
    <etapas>
        <etapa>
            <titulo>Dirigir-se á Ciretran ou Pav</titulo>
            <descricao><![CDATA[Dirigir-se á Ciretran ou Pav (Posto de Atendimento Veicular), tendo em mãos os documentos necessários para solicitar o serviço desejado]]></descricao>
            <documentos>
                <default>
                    <item>CNH - Carteira Nacional de Habilitação</item>
                    <item>Notificação de Instauração de Procedimento Administrativo de Suspensão do Direito de Dirigir</item>
                    <item>Requerimento preenchido e assinado</item>
                    <item>Documentação do Condutor (quando solicitado por terceiros)</item>
                    <item>Procuração Pública (quando solicitado por terceiros)</item>
                </default>
            </documentos>
            <custos>
                <default/>
            </custos>
            <canais-de-prestacao>
